{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CloudFlareSettings": {
    "BaseURL": "https://api.cloudflare.com/client/v4",
    "BearerToken": "YOUR_BEARER_TOKEN",
    "SyncDNSRecord": false,
    "DNS_Record_ID": "YOUR_DNS_RECORD_ID",
    "DNS_Record_Name": "YOUR_DOMAIN_NAME",
    "ZoneID": "YOUR_ZONE_ID"
  },
  "EmailSettings": {
    "ReportTo": "",
    "OutgoingServer": {
      "Server": "",
      "UserName": "",
      "FromAddress": "",
      "Password": "",
      "Port": 587,
      "HasSSL": true,
      "SSLVersion": "TSL"
    }
  }
}
